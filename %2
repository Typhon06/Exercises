import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner finder = new Scanner(System.in);
        System.out.println("Â¿Hasta que numero lo quieres?");
        int nUsuario = finder.nextInt();
        int i;
        for (i = 0; i <= nUsuario; i++) {
            if (i % 2 == 0) {
                System.out.println(i);
            }
        }

    }
}
